                  "plum",
                  "powderblue",
                  "purple",
                  "rebeccapurple",
                  "red",
                  "rosybrown",
                  "royalblue",
                  "saddlebrown",
                  "salmon",
                  "sandybrown",
                  "seagreen",
                  "seashell",
