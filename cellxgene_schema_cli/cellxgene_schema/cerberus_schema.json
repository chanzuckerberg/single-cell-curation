                  "thistle",
                  "tomato",
                  "turquoise",
                  "violet",
                  "wheat",
                  "white",
                  "whitesmoke",
                  "yellow",
                  "yellowgreen"
                ],
                "type": "string"
              }
            }
          ],
          "check_with": "obs_category_mapping",
          "type": "ndarray"
        }
      }
    ],
    "type": "dict"
  },
  "var": {
    "attributes_schema": {
      "feature_is_filtered": {
        "check_with": "feature_is_filtered",
        "dtype": {
          "type": "boolean"
        },
        "required": true
      },
      "shape": {
        "index_schemas": {
          "0": {
            "warn": {
              "min": 2000
            }
          }
        }
      }
    },
    "index": {
      "check_with": [
        "unique",
        "feature_id"
      ],
      "dtype": {
        "type": "string"
      }
    },
    "required": true,
    "type": "dataframe"
  },
  "varm": {
    "check_with": "annotation_mapping"
  },
  "varp": {
    "check_with": "annotation_mapping"
  }
}
